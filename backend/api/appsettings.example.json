{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Authentication": {
    "JwtKey": "TYPE_YOUR_JWT_PRIVATE_KEY",
    "JwtExpireDays": 15,
    "JwtIssuer": "https://yourapi.example.com"
  },
  "AllowedHosts": "*",
  "ImagesPath": "Images",

  "ConnectionStrings": {
    "IoTDbConnection": "Server=YOUR_DB_SERVER;Database=IoTManagementSystemDb;Trusted_Connection=True;"
  },
  "MqttSettings": {
    "ClientId": "your-client-id",
    "Broker": "your-broker-url",
    "MqttUsername": "your-mqtt-username",
    "MqttPassword": "your-mqtt-password",
    "Port": 8883,
    "CleanSession": true
  }
}
